<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{{name}}'s Resume</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
            padding: 0;
        }
        h1, h2, h3 {
            color: #333;
        }
        .section {
            margin-bottom: 20px;
        }
        .skills {
            list-style-type: none;
            padding: 0;
        }
        .skills li {
            background: #f0f0f0;
            margin: 5px 0;
            padding: 10px;
            border-radius: 5px;
        }
        .item-list {
            margin: 0;
            padding: 0;
            list-style: none;
        }
        .item-list li {
            margin-bottom: 10px;
        }
        .sub-list {
            margin-left: 20px;
        }
    </style>
</head>
<body>
    <h1>{{name}}</h1>
    <p>Email: {{email}}</p>
    <p>Phone: {{phone}}</p>
    {{#if address}}<p>Address: {{address}}</p>{{/if}}

    {{#if objective}}
    <div class="section">
        <h2>Objective</h2>
        <p>{{objective}}</p>
    </div>
    {{/if}}

    {{#if education}}
    <div class="section">
        <h2>Education</h2>
        <ul class="item-list">
            {{#each education}}
            <li>
                <strong>{{degree}}</strong>, {{institution}} ({{year}})
            </li>
            {{/each}}
        </ul>
    </div>
    {{/if}}

    {{#if experience}}
    <div class="section">
        <h2>Experience</h2>
        <ul class="item-list">
            {{#each experience}}
            <li>
                <strong>{{jobTitle}}</strong> at {{company}} ({{startDate}} - {{#if endDate}}{{endDate}}{{else}}Present{{/if}})
                {{#if responsibilities}}
                <ul class="sub-list">
                    {{#each responsibilities}}
                    <li>{{this}}</li>
                    {{/each}}
                </ul>
                {{/if}}
                {{#if projects}}
                <div class="sub-list">
                    <strong>Projects:</strong>
                    <ul>
                        {{#each projects}}
                        <li>
                            <strong>{{name}}</strong>{{#if description}}: {{description}}{{/if}}
                        </li>
                        {{/each}}
                    </ul>
                </div>
                {{/if}}
                {{#if achievements}}
                <div class="sub-list">
                    <strong>Achievements:</strong>
                    <ul>
                        {{#each achievements}}
                        <li>{{this}}</li>
                        {{/each}}
                    </ul>
                </div>
                {{/if}}
            </li>
            {{/each}}
        </ul>
    </div>
    {{/if}}

    {{#if skills}}
    <div class="section">
        <h2>Skills</h2>
        <ul class="skills">
            {{#each skills}}
                <li>{{this}}</li>
            {{/each}}
        </ul>
    </div>
    {{/if}}

    {{#if certifications}}
    <div class="section">
        <h2>Certifications</h2>
        <ul class="item-list">
            {{#each certifications}}
            <li>
                <strong>{{name}}</strong> - {{issuingOrganization}} ({{year}})
            </li>
            {{/each}}
        </ul>
    </div>
    {{/if}}

    {{#if languages}}
    <div class="section">
        <h2>Languages</h2>
        <ul class="item-list">
            {{#each languages}}
            <li>
                {{language}} ({{proficiency}})
            </li>
            {{/each}}
        </ul>
    </div>
    {{/if}}

    {{#if projects}}
    <div class="section">
        <h2>Projects</h2>
        <ul class="item-list">
            {{#each projects}}
            <li>
                <strong>{{name}}</strong> ({{year}})<br>
                {{description}}<br>
                {{#if link}}<a href="{{link}}">{{link}}</a><br>{{/if}}
                <em>Technologies:</em> {{#each technologies}}{{this}}{{#unless @last}}, {{/unless}}{{/each}}
            </li>
            {{/each}}
        </ul>
    </div>
    {{/if}}

    {{#if references}}
    <div class="section">
        <h2>References</h2>
        <ul class="item-list">
            {{#each references}}
            <li>
                <strong>{{name}}</strong> ({{relationship}})<br>
                Email: {{email}}<br>
                Contact: {{contact}}
            </li>
            {{/each}}
        </ul>
    </div>
    {{/if}}
</body>
</html>