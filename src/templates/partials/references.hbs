{{#if references}}
    <div class="section">
        <h3>References</h3>
        <ul class="item-list">
            {{#each references}}
            <li>
                <strong>{{name}}</strong> ({{relationship}})<br>
                Email: {{email}}<br>
                Contact: {{contact}}
            </li>
            {{/each}}
        </ul>
    </div>
{{/if}}